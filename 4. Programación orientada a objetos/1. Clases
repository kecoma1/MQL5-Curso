class Persona {
   string            nombre;     // nombre 
   string            apellido;    // apellido
   
public:
   Persona(string n, string a) {
      this.nombre = n;
      this.apellido = a;
   }
   
   string ToString() {
      return nombre+" "+apellido;
   }
};
 
void OnInit() {

   Person p1("Kevin", "De la Coba");
   Person p2("Optimus", "Prime");
   Person p3("Asus", "VivoBook");

   Print("First person: "+ p1.ToString());
   Print("Second person: "+ p2.ToString());
   Print("Third person: "+ p3.ToString());
   
}
