
void OnInit() {
   // De forma muy similar a como usabamos los valores enteros, usamos los valores decimales
   double a = 0.12;
   
   Print(a);
   
   a = 1.34;
   
   Print(a);
   
   // Al declarar una variable float es importante poner siempre una f al final.
   // La si no ponemos la f estamos intentando asignar un valor que ocupa más de lo que debería ocupar un float.
   float b = 0.32f;
   
   Print(b);
   
   // Vamos a probar el código en el probador de estrategias
}